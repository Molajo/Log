{
    "name": "molajo/log",
    "type": "molajo-package",
    "description": "Molajo Logs: Basic PSR-3 compliant Log Package for PHP applications",
    "keywords": ["Log", "Molajo", "CommonApi"],
    "homepage": "http://github.com/Molajo/Log",
    "license": "MIT",
    "authors": [
        {
            "name": "Amy Stephen",
            "email": "AmyStephen@Molajo.org",
            "homepage": "http://AmyStephen.com"
        }
    ],
    "require": {
        "psr/log": "dev-master",
        "commonapi/controller": "dev-master",
        "commonapi/exception": "dev-master"
    },
    "support": {
        "log": "hello@molajo.org",
        "irc": "irc://irc.freenode.org/molajo"
    },
    "autoload": {
        "psr-4": {
            "Molajo\\Controller\\": "Controller/",
            "Molajo\\Log\\": "Source/",
            "Molajo\\Factories\\": "Factories/"
        }
    },

    "archive": {
        "exclude": ["/.dev"]
    },

    "minimum-stability": "dev"
}
